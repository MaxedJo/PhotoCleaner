<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="457.0" prefWidth="513.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.maxed.photocleaner.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="pathToFiles" alignment="CENTER" contentDisplay="CENTER" prefHeight="22.0" prefWidth="482.0" text="Путь к файлам">
      <font>
         <Font size="15.0" />
      </font>
   </Label>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <ListView fx:id="list" prefHeight="200.0" prefWidth="200.0" />
         <ListView fx:id="filteredList" prefHeight="200.0" prefWidth="200.0">
            <HBox.margin>
               <Insets left="30.0" />
            </HBox.margin>
         </ListView>
      </children>
   </HBox>
    <Label fx:id="text" text="Введите путь" />
    <TextField fx:id="input" prefHeight="0.0" prefWidth="410.0" />
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <VBox alignment="CENTER" prefHeight="100.0" prefWidth="127.0">
            <children>
               <Label text="Эталон" />
               <TextField fx:id="mainExpansive" />
            </children>
         </VBox>
         <VBox alignment="CENTER" prefHeight="100.0" prefWidth="118.0">
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin>
            <children>
               <Label text="Обработка" />
               <TextField fx:id="secondaryExpansive" />
            </children>
         </VBox>
      </children>
   </HBox>
    <Button fx:id="test" onAction="#onOpenButtonClick" text="Открыть" />
</VBox>
